#!/usr/bin/python

import sys

from clone import do_clone
from clone import do_pull

command_table = {
        "clone": do_clone,
        "checkout": do_clone,
        "co": do_clone,
        "pull": do_pull,
        "update": do_pull,
        }

def main():
    command_table[sys.argv[1]](*sys.argv[2:])

if __name__ == "__main__":
    main()
